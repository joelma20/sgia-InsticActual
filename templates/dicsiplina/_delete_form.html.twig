<form method="post" action="{{ path('dicsiplina_delete', {'id': dicsiplina.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dicsiplina.id) }}">
    <button class="btn btn-sm btn-danger"><i class="fa-trash-trash"></i>Eliminar</button>
</form>



